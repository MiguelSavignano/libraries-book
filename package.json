{
  "name": "package-versions-managers",
  "version": "2.4.1",
  "description": "Useful tool to get versions (node or npm package) from repo(s) of Github user/org",
  "main": "build/index.js",
  "bin": {
    "grt": "./build/app.js"
  },
  "scripts": {
    "clean": "",
    "build": "rimraf ./build && tsc",
    "build:watch": "tsc -w",
    "start:prod": "ts-node ./src/app.ts",
    "start": "node -r ts-node/register ./src/graphql/app.ts",
    "dev": "nodemon",
    "lint": "tslint -p .",
    "lint:fix": "tslint -p . --fix"
  },
  "license": "ISC",
  "keywords": [
    "github"
  ],
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run start",
    "ext": "ts"
  },
  "dependencies": {
    "@apollo/client": "^3.1.1",
    "@magna_shogun/catch-decorator": "^1.0.1",
    "@octokit/graphql": "^4.5.3",
    "@octokit/plugin-retry": "^2.2.0",
    "@octokit/rest": "^18.0.3",
    "@types/fs-extra": "^9.0.1",
    "@yarnpkg/lockfile": "^1.1.0",
    "apollo-server": "^2.16.1",
    "apollo-server-testing": "^2.16.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "catch-decorator": "^2.0.0",
    "chalk": "^2.4.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "gemfile": "^1.1.0",
    "graphql": "^15.3.0",
    "inquirer": "^6.3.1",
    "jest": "^26.2.2",
    "json2csv": "^5.0.1",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "slugify": "^1.4.0",
    "ts-node": "^8.10.2",
    "uglify-js": "^3.10.1",
    "yargs": "^13.2.4"
  },
  "engines": {
    "node": ">= 8.16.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.53",
    "nodemon": "^2.0.4",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
